
@{
    ViewBag.Title = "程序版本管理系统";
    Layout = "~/Areas/Version/Views/Shared/_Layout.cshtml";
}


@section Description{
    欢迎登录程序版本管理系统
    }


@section ButtonHead{
    <div class="title-action">
        <a href="/Version/Program/Create" class="btn btn-primary btn-outline" data-toggle="modal" data-target="#Modal"><i class="fa fa-github"></i>添加程序</a>
    </div>
    }


<div class="row">
    <div class="col-xs-6">
        @{ 
            Html.RenderAction("Widget", "Program");
        }
    </div>
    <div class="col-xs-6">
        @{ 
            Html.RenderAction("Widget", "Version");
        }
    </div>
</div>