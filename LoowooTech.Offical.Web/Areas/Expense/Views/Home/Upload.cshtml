@{ 
    Layout = null;
}


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <h4 class="modal-title">上传电子发票文件</h4>
</div>

<form class="form-horizontal" method="post" action="/Expense/Home/UploadFile" Url="" enctype="multipart/form-data">
    <div class="modal-body">
        <div class="form-group">
            <label class="control-label col-xs-3">文件：</label>
            <div class="col-xs-8">
                <input type="hidden" name="SheetId" value="@(Request.QueryString["SheetId"])" />
                <input type="file" class="form-control" name="file" />
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i>上传</button>
        <button type="button" class="btn btn-white" data-dismiss="modal"><i class="fa fa-times"></i> 取消</button>
    </div>
</form>
